{

  "name": "flutter-bloc-feature-generator",
  "displayName": "Flutter BLoC Feature Generator",
  "description": "Membuat struktur fitur BLoC dengan cepat melalui menu klik kanan.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "publisher": "khoirul mustofa",
  "repository": {
    "type": "git",
    "url": "https://github.com/khoirul-mustofa/flutter-bloc-generate-feature.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:flutter-bloc-generator.createFeature"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "flutter-bloc-generator.createFeature",
        "title": "New BLoC Feature"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "explorerResourceIsFolder",
          "command": "flutter-bloc-generator.createFeature",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.x",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
